{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"ConnectionStrings": {
		"DatabaseConnection": "Server=localhost;Database=arquisoft;User=arquisoft;Password=arquisoft;"
	},
	"KafkaBroker": {
		"Server": "kafka-broker:9094",
		"Topic": "interactions"
	},
	"Urls": "http://localhost:8089",
	"eureka": {
		"client": {
			"serviceUrl": "http://localhost:8761/eureka",
			"shouldFetchRegistry": "false",
			"shouldRegisterWithEureka": true,
			"validateCertificates": false
		},
		"instance": {
			"appName": "Recommendations-Microservice",
			"port": "8089",
			"ipAddress": "recommendationsmicroservice",
			"preferIpAddress": true
		}
	}
}